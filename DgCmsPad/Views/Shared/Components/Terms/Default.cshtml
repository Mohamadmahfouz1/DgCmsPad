@model IEnumerable<Term>


<ul class="list-group">
    <li class="list-group-item">
        <a class="nav-link text-dark" asp-area="" asp-controller="Terms"asp-action="Index">Categories</a>

    </li>



@foreach (var item in Model)
{
    <li class="list-group-item">
        <a class="nav-link text-dark" asp-area="" asp-controller="Terms" asp-action="PostsByTerm" asp-route-categorySlug="@item.Slug">@item.Name</a>
    </li>
}

</ul>