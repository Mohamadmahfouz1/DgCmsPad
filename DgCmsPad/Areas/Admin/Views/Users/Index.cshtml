@model IEnumerable<AppUser>

@{
    ViewData["Title"] = "Users";
}

                      <h1>Users</h1>


<table class="table">
    <thead>
        <tr>
           
            <th>
               Username
            </th>
            <th>
                Email
            </th>
            <th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
           
            <td>
                @item.UserName
            </td>
            <td>
                @item.Email
            </td>
       
        </tr>
}
    </tbody>
</table>
